{
  "name": "myowncloud-backend",
  "version": "1.0.0",
  "description": "AI-powered file storage backend with intelligent search and categorization",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "db:generate": "npx prisma generate",
    "db:migrate": "npx prisma migrate dev",
    "db:deploy": "npx prisma migrate deploy",
    "db:seed": "node prisma/seed.js",
    "db:reset": "npx prisma migrate reset",
    "docker:build": "docker build -t myowncloud-backend .",
    "docker:run": "docker run -p 8000:8000 myowncloud-backend",
    "health": "curl -f http://localhost:8000/ || exit 1"
  },
  "keywords": ["file-storage", "ai", "groq", "search", "cloud"],
  "author": "Pavan Kumar V",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.12.0",
    "archiver": "^7.0.1",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "file-type": "^16.5.4",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "node-fetch": "^2.7.0",
    "openai": "^5.12.2",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^1.1.1",
    "pdf-poppler": "^0.2.1",
    "sharp": "^0.34.3",
    "tesseract.js": "^6.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.1.10",
    "prisma": "^6.12.0"
  }
}
